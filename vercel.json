{
  "version": 2,
  "builds": [
    { "src": "*.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/abdullah", "dest": "/abdullah.js" },
    { "src": "/abdullahpass", "dest": "/abdullahpass.js" },
    { "src": "/hamid", "dest": "/hamid.js" },
    { "src": "/hamidpass", "dest": "/hamidpass.js" },
    { "src": "/jahangir", "dest": "/jahangir.js" },
    { "src": "/jahangirpass", "dest": "/jahangirpass.js" },
    { "src": "/rashid2", "dest": "/rashid2.js" }, 
    { "src": "/rashid2pass", "dest": "/rashid2pass.js" },
    { "src": "/warispass", "dest": "/warispass.js" },
    { "src": "/fahadpass", "dest": "/fahadpass.js" }
  ]
}
